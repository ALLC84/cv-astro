---
import PartingLine from '@components/PartingLine/index.astro';

import HeaderContent from './HeaderContent.astro';
import WorkExperience from './WorkExperience.astro';
import Education from './Education.astro';
import Courses from './Courses.astro';

import contactData from '@data/contact.json';
import coursesData from '@data/courses.json';
import workExperienceData from '@data/workExperience.json';
import educationData from '@data/education.js';
---

<!--
    |-----------------------------------------------
    |  HEADER
    |-----------------------------------------------
    -->
<HeaderContent data={contactData} />
<PartingLine weight={0.01} width={100} mt={1} />

<!--
    |-----------------------------------------------
    |  EXPERIENCIA LABORAL
    |-----------------------------------------------
    -->
<WorkExperience data={workExperienceData}/>
<PartingLine weight={0.01} width={100} mt={1} />

<!--
    |-----------------------------------------------
    |  EDUCACION
    |-----------------------------------------------
    -->
<Education data={educationData} />
<PartingLine weight={0.01} width={100} mt={1} />

<!--
    |-----------------------------------------------
    |  CURSOS
    |-----------------------------------------------
    -->
<Courses data={coursesData} />
<PartingLine weight={0.01} width={100} mt={1} />
