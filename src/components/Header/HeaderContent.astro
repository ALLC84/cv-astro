---
import TitleSection from '@components/TitleSection/index.astro';

interface DataContact {
  name: string;
  profession: string;
  about: string;
}

interface Props {
  data: DataContact;
}

const { data } = Astro.props;
---
  
<header>
  <TitleSection
    tag="h2"
    size={2.8}
    weight={800}
    text={data.name}
  />

  <TitleSection
    tag="h1"
    text={data.profession}
    weight={600}
    mt={1}
  />
  
  <p>{data.about}</p>
</header>